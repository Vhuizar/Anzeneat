
<div class="container p-4" id="fondop">
    <div class="row">
        <div class="col md-4 text-center">
            <div class=" card text-left">
                <a href="/links/useredit/{{user.id}}" class="btn btn-primary"> EDITAR</a>
                <div class="card-body">

                    <form class="form-horizontal">
                        <div class="row">
                            <div class="col-md-8" id="formp">
                                <br>
                                <div class="form-group">
                                    <label class="col-sm-4 control-label" id="text">Nombre Competo</label>
                                    <div class="col-sm-8">
                                        <input type="text" readonly id="addimput" class="form-control"
                                            value="{{user.name}} {{user.last_name}}">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-4 control-label" id="text">Email</label>
                                    <div class="col-sm-8">
                                        <input type="text" readonly id="addimput" class="form-control" value="{{user.email}}">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-4 control-label" id="text">Rol Usuario</label>
                                    <div class="col-sm-8">
                                        <input type="text" readonly id="addimput" class="form-control" value="{{user.rol}}">
                                    </div>
                                </div>
                            </div>
                            <br>
                        </div>
                    </form>
                    <br>
                </div>
            </div>
        </div>
    </div>
</div>